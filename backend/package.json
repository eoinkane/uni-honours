{
  "name": "backend",
  "version": "0.1.0",
  "bin": {
    "backend": "bin/backend.js"
  },
  "scripts": {
    "build": "tsc && npm -s run build-handler-lambda",
    "build-handler-lambda": "rm -rf build/handler-lambda && mkdir build/handler-lambda && pip install --platform manylinux2014_x86_64  --only-binary=:all: --target=build/handler-lambda -r code/handler-lambda/requirements.txt && cp -r code/handler-lambda/src build/handler-lambda/",
    "cdk": "cdk",
    "clean": "rm -rf ./build ./cdk.out",
    "deploy": "cdk deploy --app cdk.out",
    "init": "mkdir build",
    "synth": "cdk synth",
    "test": "jest",
    "watch": "tsc -w"
  },
  "devDependencies": {
    "@aws-cdk/assertions": "1.198.0",
    "@types/babel__traverse": "7.18.2",
    "@types/jest": "^27.5.0",
    "@types/node": "10.17.27",
    "@types/prettier": "2.6.0",
    "aws-cdk": "1.198.0",
    "jest": "^27.5.1",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.8.1",
    "typescript": "~3.9.7"
  },
  "dependencies": {
    "@aws-cdk/aws-apigateway": "^1.198.0",
    "@aws-cdk/aws-lambda": "^1.198.0",
    "@aws-cdk/aws-route53": "^1.198.0",
    "@aws-cdk/aws-s3": "^1.198.0",
    "@aws-cdk/core": "1.198.0",
    "dotenv": "^16.0.3",
    "source-map-support": "^0.5.21"
  }
}
